<Window x:Class="Exp1.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Результат" Height="300" Width="600">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="10,0,10,30">
        <Grid Name="logGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"  HorizontalAlignment="Left" Margin="10,10,0,0" Name="ResultBlock" Text="Результат:" VerticalAlignment="Top" />
        <TextBlock Grid.Row="1"  HorizontalAlignment="Left" Margin="10,10,0,0" Text="Лог:" VerticalAlignment="Top" />
        
    </Grid>
        </ScrollViewer>
        <Button Content="Закрыть" Grid.Row="1" Height="23" HorizontalAlignment="Right" Margin="0,0,5,5" Name="CloseButton" VerticalAlignment="Bottom" Width="75" Click="CloseButton_Click" />
    </Grid>
</Window>
